#!groovy

properties([disableConcurrentBuilds()])

pipeline{
	agent {
		label 'master'
	}
	triggers {pollSCM('* * * * *')}
	options {
		buildDiscarder(logRotator(numToKeepStr:'10',artifactNumToKeepStr:'10'))
		timestamps()
	}
	stages {
		stage('build'){
			steps {
				script{
                  			def output = sh(returnStdout: true, script: 'ls | grep LyskovoChat').trim()
                  			echo "Output: '${output}'"
					if(output == "LyskovoChat"){
						echo "i am find"
					}else{
						echo "i am not find"
					}
				}
			}
		}
	}
}
