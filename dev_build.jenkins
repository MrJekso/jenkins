#!groovy

properties([disableConcurrentBuilds()])

pipeline{
	agent {
		label 'master'
	}
	triggers {pollSCM('* * * * *')}
	options {
		buildDiscarder(logRotator(numToKeepStr:'10',artifactNumToKeepStr:'10'))
		timestamps()
	}
	stages {
		stage('build'){
			steps {
				sh 'cd ~'
				sh 'git clone git@github.com:MrJekso/LyskovoChat.git'
				sh 'ls -la'
			}
		}
	}
}
